<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Tom치코 Bouda</Authors>
    <Company>TomLabs</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>Slack, webhook, helper, SlackClient, Post, Message</PackageTags>
    <RepositoryType>Git</RepositoryType>
    <Description>Simple library for sending messages into Slack channels using Webhook.</Description>
    <PackageLicenseUrl>https://github.com/TomasBouda/SlackHook/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/TomasBouda/SlackHook</PackageProjectUrl>
    <RepositoryUrl>https://github.com/TomasBouda/SlackHook</RepositoryUrl>
    <Copyright>Tom치코 Bouda</Copyright>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageIconUrl>https://raw.githubusercontent.com/TomasBouda/SlackHook/master/icon.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.1" />
  </ItemGroup>

</Project>
